@using MvcAppWeek4.Models;
@model List<Student>
           
@{
    ViewBag.Title = "ShowStudents";
}

<h2>ShowStudents</h2>
@{
    if (Model.Count == 0)
    {
        <div>geen studenten gevonden</div>
    } 
    else 
    {
        foreach (Student stud in Model)
        {
            <div>@stud.studentnaam</div>
        }
    }
}